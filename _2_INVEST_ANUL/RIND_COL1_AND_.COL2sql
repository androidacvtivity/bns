SELECT
  
   
  D.RIND, 
  SUM(D.COL1) AS COL1,
  SUM(D.COL2) AS COL2
  
   
  
 
  
  
FROM 
  CIS2.VW_DATA_ALL D
  

     
WHERE
 (D.FORM=:pFORM) AND
 (D.FORM_VERS=:pFORM_VERS) AND
 (:pID_MDTABLE=:pID_MDTABLE) AND
 (D.CUATM_FULL LIKE '%'||:pCOD_CUATM||';%') AND   
  D.PERIOADA IN :pPERIOADA AND
  D.FORM = 8 AND
  D.CAPITOL = 323 
  AND D.RIND IN ('440','410','420','430','441','442','450')
    
GROUP BY
  D.RIND
 
ORDER BY 
 D.RIND